{
  "name": "keiba_api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "2.536.0",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "iconv-lite": "^0.5.1",
    "moment": "^2.24.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "dev": "nodemon -L",
    "build": "webpack --config ./webpack/prod.config.js",
